{
  "manifest_version": 1,
  "name": "echo-speaks-server",
  "license": "MIT",
  "name_pretty": "Echo Speaks",
  "version": "0.1.0",
  "last_update": "2022-12-03T04:43:09+00:00",
  "description": "Echo Speaks Server to provide continuous cookie refresh",
  "keywords": [
    "auto:container",
    "auto:echo-speaks-server",
    "auto:user-integration"
  ],
  "quality": null,
  "contributors": [
    {
      "name": "@parish99",
      "email": "parish99@hotmail.com"
    }
  ],
  "copyright": [
    "Set User"
  ],
  "code_owners": [
    "@parish99"
  ],
  "iot_class": null,
  "target_user": "oh-la",
  "home": "default",
  "data_folder": "default",
  "container": {
    "name": "{{manifest.name}}",
    "license": "MIT",
    "repo": "tonesto7/echo-speaks-server:latest",
    "port_map": [
      "127.0.0.1:{{manifest.container.http_port}}:8091"
    ],
    "http_port": 10945,
    "export_serial": false,
    "folder_map": [
      "$HOME/data/{{manifest.name}}/root:/root",
      "/etc/ssl/certs:/etc/ssl/certs:ro",
      "/usr/local/share/ca-certificates:/usr/local/share/ca-certificates:ro"
    ],
    "env_map": [
      "{{env_map.node_extra_ca_certs}},PUID=1000,PGID=1000"
    ]
  },
  "systemd": {
    "generate_template": "default",
    "update_before_start": false,
    "systemd_additional_before": "",
    "systemd_additional_after": "",
    "systemd_restart": "always",
    "systemd_restart_sec": 30,
    "systemd_timeout_stop_sec": 70,
    "systemd_timeout_start_sec": "10m",
    "systemd_exec_start_pre": "",
    "systemd_exec_start_post": "",
    "systemd_exec_stop_post": "",
    "podman_timeout_stop_sec": 10
  },
  "nginx": {
    "config_template": "services/podman_default_no_path",
    "config_enabled": [
      "services-enabled"
    ],
    "ports": [],
    "ui_url": "/pods/direct/{{manifest.name}}/",
    "test_url": "/pods/direct/{{manifest.name}}/",
    "sso_urls": [
      "/pods/direct/{{manifest.name}}/"
    ],
    "unprotected_urls": []
  },
  "data": {
    "owner_user": "default",
    "owner_group": "oh-la",
    "chown_from_root": true
  },
  "secrets": {},
  "user_input": {},
  "variables": {},
  "requirements": {
    "marjebian": ">=1.8.2.0727",
    "oll": {},
    "cui": {},
    "apt": {},
    "pip3": {},
    "npm": {}
  },
  "mqtt": {},
  "supported_brands": {},
  "discovery": {},
  "manifest_sha1": "f9662a412067f8ea849e2e550c980ce13b7041e6"
}